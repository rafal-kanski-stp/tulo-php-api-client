{
    "name": "rafal-kanski-stp/tulo-php-api-client",
    "license": "MIT",
    "description": "This is PHP Client for Tulo API",
    "type": "library",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Tulo\\Api\\": "src/Tulo/Api"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=7.2",
        "league/oauth2-client": "^2.4",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^6.0",
        "phpmd/phpmd": "^2.7",
        "squizlabs/php_codesniffer": "^3.5",
        "phing/phing": "^2.16",
        "sebastian/phpcpd": "^4.1"
    }
}
